<%= form_with(model: @review, url: review_path(@review), method: :patch) do |form| %>
  <!-- Form fields go here -->
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :content %>
  <%= form.text_area :content %>

  <%= form.label :images %>
  <%= form.file_field :images, multiple: true %>

  <%= form.submit "Update Review" %>
<% end %>


<%= link_to 'Back', reviews_path %>
