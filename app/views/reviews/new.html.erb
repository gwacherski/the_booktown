<%# renderizar o form javascript? %>

<%= form_for @review, remote: true do |f| %>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>

  <div class="field">
    <%= f.label :rating %><br>
    <%= f.number_field :rating %>
    </div>

    <div class="field">
      <%= f.label :images %><br>
      <%= f.file_field :images, multiple: true %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>

  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
